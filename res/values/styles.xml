    <!-- 按钮风格 -->
    <style name="dachezuche_btn">
        <item name="android:background">@drawable/dachezuche_button_selector_normal</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/normalTextSize</item>
    </style>

